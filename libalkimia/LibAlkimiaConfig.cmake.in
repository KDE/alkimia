@PACKAGE_INIT@

find_package(KDE4 REQUIRED)
find_package(GMP REQUIRED)

include("${CMAKE_CURRENT_LIST_DIR}/LibAlkimiaTargets.cmake")

set( LIBALKIMIA_VERSION "@ALKIMIA_LIB_VERSION@" )
set_and_check( LIBALKIMIA_INCLUDE_DIR "@INCLUDE_INSTALL_DIR@")

set( LIBALKIMIA_LIBRARIES
  Alkimia::alkimia
  ${QT_QTCORE_LIBRARY}
  ${QT_QTDBUS_LIBRARY}
  ${GMP_LIBRARIES}
)

# Does not comply with common naming sheme but set for backwards compatibility
set( LIBALKIMIA_LIBRARY ${LIBALKIMIA_LIBRARIES} )

message(STATUS "Found alkimia: ${LIBALKIMIA_INCLUDE_DIR}")
