@PACKAGE_INIT@

list(APPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")

if("@CMAKE_DIR_PREFIX@" STREQUAL "Qt4")
    find_package(Qt4 COMPONENTS QtCore QtDBus)
else()
    find_dependency(Qt5Core)
    find_dependency(Qt5DBus)
endif()
find_dependency(GMP)

include("${CMAKE_CURRENT_LIST_DIR}/LibAlkimia@CMAKE_DIR_PREFIX@Targets.cmake")

set_and_check(LIBALKIMIA_INCLUDE_DIR "@PACKAGE_INCLUDE_INSTALL_DIR@")
message(STATUS "Found LibAlkimia: ${CMAKE_CURRENT_LIST_DIR}/LibAlkimia@CMAKE_DIR_PREFIX@Config.cmake (found version \"@ALKIMIA_LIB_VERSION@\")")
